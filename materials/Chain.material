// Basic shader programs for OpenGL 3.3 core compatibility
vertex_program basic_color_vp glsl
{
  source shaders/basic_color.vert
}

fragment_program basic_color_fp glsl
{
  source shaders/basic_color.frag
}

// Simple shader programs for Chain materials
vertex_program chain_simple_color_vp glsl
{
  source shaders/simple_color.vert
}

fragment_program chain_simple_color_fp glsl
{
  source shaders/simple_color.frag
}

material Chain/Wheel
{
  technique
  {
    scheme default
    pass
    {
      texture_unit
      {
        texture wheel.jpg
        rotate 90
        filtering trilinear
      }
    }
  }
  technique
  {
    scheme ShaderGeneratorDefaultScheme
    pass
    {
      texture_unit
      {
        texture wheel.jpg
        rotate 90
        filtering trilinear
      }
    }
  }
}

material Chain/DryGround
{
  technique
  {
    scheme default
    pass
    {
      texture_unit
      {
        texture ground.jpg
        scale 0.15 0.15
        filtering trilinear
      }
    }
  }
  technique
  {
    scheme ShaderGeneratorDefaultScheme
    pass
    {
      texture_unit
      {
        texture ground.jpg
        scale 0.15 0.15
        filtering trilinear
      }
    }
