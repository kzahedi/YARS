material Chain/Circuit/Body/Green
{
  technique
  {
    scheme default
    pass
    {
      texture_unit
      {
        texture CBT.jpg
        scale 0.5 2
        filtering trilinear
      }
    }
  }
  technique
  {
    scheme ShaderGeneratorDefaultScheme
    pass
    {
      texture_unit
      {
        texture CBT.jpg
        scale 0.5 2
        filtering trilinear
      }
    }
  }
}

material Chain/Circuit/Top/Green
{
  technique
  {
    pass
    {
      vertex_program_ref chain_simple_color_vp
      fragment_program_ref chain_simple_color_fp

      vertex_program_ref_params
      {
        param_named_auto worldViewProjMatrix worldviewproj_matrix
      }

      fragment_program_ref_params
      {
        param_named color float4 0.0 0.8 0.0 1.0
      }
    }
  }
}

